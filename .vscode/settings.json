{
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.compilerPath": "/usr/bin/g++",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**",
        "/opt/homebrew/include/**",
        "/opt/homebrew/Cellar/givaro/*/include",
        "/opt/homebrew/opt/gmp/include",
        "/usr/local/include"
    ],
    "C_Cpp.default.defines": [],
    "C_Cpp.default.intelliSenseMode": "macos-clang-arm64",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.errorSquiggles": "enabled",
    "files.associations": {
        "*.h": "cpp",
        "*.hpp": "cpp",
        "*.cpp": "cpp",
        "iostream": "cpp",
        "vector": "cpp",
        "string": "cpp",
        "modular.h": "cpp",
        "gfq.h": "cpp",
        "montgomery.h": "cpp",
        "algorithm": "cpp",
        "cmath": "cpp",
        "__bit_reference": "cpp",
        "__locale": "cpp",
        "__split_buffer": "cpp",
        "__verbose_abort": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "locale": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "print": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "typeinfo": "cpp",
        "__hash_table": "cpp",
        "__node_handle": "cpp",
        "array": "cpp",
        "complex": "cpp",
        "cstdarg": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "execution": "cpp",
        "memory": "cpp",
        "iomanip": "cpp",
        "list": "cpp",
        "optional": "cpp",
        "queue": "cpp",
        "ratio": "cpp",
        "stack": "cpp",
        "unordered_map": "cpp",
        "variant": "cpp"
    },
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "cmake.configureOnOpen": true,
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.generator": "Unix Makefiles",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "search.exclude": {
        "**/build": true,
        "**/*.o": true,
        "**/*.obj": true,
        "**/givaro_example": true,
        "**/test_givaro": true
    },
    "files.exclude": {
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    }
}